import random

keyboard = ["a", "b", "c", "d", "e", "f", "g", "h", "i", "j", "k", "l", "m", "n", "o",
           "/", "(", ")", "1", "2", "3", "4", "5", "%", "="]
length = int(input("Introduce the length of the password"))
password = ""

while (len(password) != length):
    password = password + random.choice(keyboard)
    
print(password)  

